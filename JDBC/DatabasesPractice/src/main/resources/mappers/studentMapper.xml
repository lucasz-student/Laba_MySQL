<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.laba.solvd.database.DAO.StudentMapper">

	<select id="selectById" resultType="com.laba.solvd.database.Model.Student">
		SELECT * FROM student WHERE id = #{id}
	</select>
  
	<select id="selectAll">
		SELECT * FROM student
	</select>
	
	<delete id="delete">
		DELETE FROM student WHERE id=#{id}
	</delete>
	
	<update id="update">
		UPDATE student set name=#{name}, yearJoined=#{yearJoined}, major=#{major} WHERE id=#{id}
	</update>
	
	<insert id="create">
		INSERT INTO Student(name, yearJoined, major, Gym_id) VALUES(#{name}, #{yearJoined}, #{major}, #{Gym_id}))
	</insert>

</mapper>